<p style="color: green"><%= notice %></p>

<h1>Total Posts - <%= @posts.count %> </h1>
<%= render "import" %>
<div id="posts">
  <% @posts.each do |post| %>
    <%= render post %>
    <p>
      <%= link_to "Show this post", post %>
    </p>
  <% end %>
</div>

<%= link_to "Export", posts_path(format: 'csv') %>
<%= link_to "New post", new_post_path %>
